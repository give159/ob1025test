print(f"åå‰: {name}")
print(f"å¹´é½¢: {age}")
print(f"çµ¦æ–™: {salary}")


# --- é–¢æ•°ã®å®šç¾© ---
def greet(name: str) -> str:
    """æŒ¨æ‹¶ã‚’è¿”ã™é–¢æ•°"""
    return f"ã“ã‚“ã«ã¡ã¯ã€{name}ã•ã‚“ï¼"


message = greet("å¤ªéƒ")
print(f"\n{message}")


# --- ç°¡å˜ãªã‚¯ãƒ©ã‚¹ ---
class Person:
    """äººé–“ã‚¯ãƒ©ã‚¹ï¼ˆç·´ç¿’ç”¨ï¼‰"""
    
    def __init__(self, name: str):
        """ã‚³ãƒ³ã‚¹ãƒˆãƒ©ã‚¯ã‚¿"""
        self.name = name
    
    def introduce(self):
        """è‡ªå·±ç´¹ä»‹"""
        return f"ç§ã¯{self.name}ã§ã™"


# ã‚¯ãƒ©ã‚¹ã‚’ä½¿ã£ã¦ã¿ã‚‹
person = Person("å¤ªéƒ")
print(f"\n{person.introduce()}")

print("\n" + "=" * 50 + "\n")


# ===============================================
# ã‚¹ãƒ†ãƒƒãƒ—2: employee.pyï¼ˆåŸºåº•ã‚¯ãƒ©ã‚¹ï¼‰
# ===============================================
"""
ã€ã“ã®ã‚¹ãƒ†ãƒƒãƒ—ã§å­¦ã¶ã“ã¨ã€‘
- ã‚¯ãƒ©ã‚¹ã®å®šç¾©
- ã‚³ãƒ³ã‚¹ãƒˆãƒ©ã‚¯ã‚¿ï¼ˆ__init__ï¼‰
- ãƒ¡ã‚½ãƒƒãƒ‰ã®å®šç¾©
- ãƒ‡ã‚¹ãƒˆãƒ©ã‚¯ã‚¿ï¼ˆ__del__ï¼‰
- Docstringï¼ˆãƒ‰ã‚­ãƒ¥ãƒ¡ãƒ³ãƒˆï¼‰
"""

print("=== ã‚¹ãƒ†ãƒƒãƒ—2: Employeeã‚¯ãƒ©ã‚¹ã‚’ä½œã‚‹ ===\n")


class Employee:
    """
    ç¤¾å“¡ã®åŸºåº•ã‚¯ãƒ©ã‚¹
    
    Attributes:
        _name (str): ç¤¾å“¡ã®åå‰
        _salary (int): ç¤¾å“¡ã®çµ¦æ–™
    """
    
    def __init__(self):
        """
        ã‚³ãƒ³ã‚¹ãƒˆãƒ©ã‚¯ã‚¿
        ã‚¤ãƒ³ã‚¹ã‚¿ãƒ³ã‚¹ä½œæˆæ™‚ã«è‡ªå‹•å®Ÿè¡Œã•ã‚Œã‚‹
        """
        self._name: str = ""
        self._salary: int = 0
        print(f"[ä½œæˆ] Employeeã‚¤ãƒ³ã‚¹ã‚¿ãƒ³ã‚¹ã‚’ä½œæˆã—ã¾ã—ãŸ")
    
    def get_name(self) -> str:
        """åå‰ã‚’å–å¾—"""
        return self._name
    
    def set_name(self, emp_name: str) -> None:
        """åå‰ã‚’è¨­å®š"""
        self._name = emp_name
    
    def get_salary(self) -> int:
        """çµ¦æ–™ã‚’å–å¾—"""
        return self._salary
    
    def set_salary(self, emp_salary: int) -> None:
        """çµ¦æ–™ã‚’è¨­å®š"""
        self._salary = emp_salary
    
    def __del__(self):
        """
        ãƒ‡ã‚¹ãƒˆãƒ©ã‚¯ã‚¿
        ã‚¤ãƒ³ã‚¹ã‚¿ãƒ³ã‚¹å‰Šé™¤æ™‚ã«è‡ªå‹•å®Ÿè¡Œã•ã‚Œã‚‹
        """
        print(f"[å‰Šé™¤] Employee '{self._name}' ã‚’å‰Šé™¤ã—ã¾ã—ãŸ")


# å®Ÿéš›ã«ä½¿ã£ã¦ã¿ã‚‹
print("ã€Employeeã‚¯ãƒ©ã‚¹ã®ãƒ†ã‚¹ãƒˆã€‘")
emp = Employee()
emp.set_name("å±±ç”°å¤ªéƒ")
emp.set_salary(300000)

print(f"åå‰: {emp.get_name()}")
print(f"çµ¦æ–™: {emp.get_salary()}å††")

print("\n" + "=" * 50 + "\n")


# ===============================================
# ã‚¹ãƒ†ãƒƒãƒ—3: staff.pyï¼ˆç¶™æ‰¿ï¼‰
# ===============================================
"""
ã€ã“ã®ã‚¹ãƒ†ãƒƒãƒ—ã§å­¦ã¶ã“ã¨ã€‘
- ç¶™æ‰¿ã®æ–¹æ³•
- super()ã®ä½¿ã„æ–¹
- è¦ªã‚¯ãƒ©ã‚¹ã®æ©Ÿèƒ½ã‚’ä½¿ã†
"""

print("=== ã‚¹ãƒ†ãƒƒãƒ—3: Staffã‚¯ãƒ©ã‚¹ã‚’ä½œã‚‹ï¼ˆç¶™æ‰¿ï¼‰ ===\n")


class Staff(Employee):  # â† Employee ã‚’ç¶™æ‰¿
    """
    ä¸€èˆ¬ç¤¾å“¡ã‚¯ãƒ©ã‚¹
    
    Employeeã‚’ç¶™æ‰¿ã—ã€éƒ¨ç½²æƒ…å ±ã‚’è¿½åŠ 
    
    Attributes:

# ğŸš¨ ã‚ˆãã‚ã‚‹é–“é•ã„ã¨è§£æ±ºæ–¹æ³•

æ–°äººã‚¨ãƒ³ã‚¸ãƒ‹ã‚¢ãŒå¿…ãšã¶ã¤ã‹ã‚‹å£ã¨ã€ãã®è§£æ±ºæ–¹æ³•ã‚’ã¾ã¨ã‚ã¾ã—ãŸï¼

